{
  "name": "sass-composer",
  "version": "2.0.0-beta15",
  "author": "James Newell <james@digitaledgeit.com.au>",
  "keywords": [
    "sass",
    "composer",
    "import",
    "libsass",
    "scss",
    "style",
    "node",
    "watch",
    "npm",
    "require",
    "browserify"
  ],
